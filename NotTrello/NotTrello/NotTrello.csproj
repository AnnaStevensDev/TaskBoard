<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Haley.WPF" Version="5.4.3.1" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="NotTrelloSettings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>NotTrelloSettings.settings</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="NotTrelloSettings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>NotTrelloSettings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
